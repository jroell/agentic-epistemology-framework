    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist",
    "LICENSE",
